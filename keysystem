-- =========================================================================
-- !!! BUTTON CONNECTIONS !!!
-- =========================================================================

GetKeyBtn.MouseButton1Down:Connect(function()
    GetKeyBtn.Text = "LOADING..."
    
    task.spawn(function()
        local success, result = copyLink()
        
        if success then
            showNotification("Link Copied!", "Open browser and paste the link", Color3.fromRGB(0, 255, 100), 4)
            GetKeyBtn.Text = "COPIED!"
            task.wait(2)
            GetKeyBtn.Text = "GET KEY"
        else
            showNotification("Error", result or "Failed to get link. Try again.", Color3.fromRGB(255, 50, 50), 3)
            GetKeyBtn.Text = "GET KEY"
        end
    end)
end)

CheckKeyBtn.MouseButton1Down:Connect(function()
    local key = TextBox.Text
    
    if key == "" or key == nil then
        showNotification("Error", "Please enter a key first!", Color3.fromRGB(255, 50, 50), 3)
        return
    end
    
    CheckKeyBtn.Text = "CHECKING..."
    
    task.spawn(function()
        local success, message = verifyKey(key)
        
        if success then
            -- Save valid key
            saveKey(key)
            
            showNotification("Success!", "Key verified! Loading script...", Color3.fromRGB(0, 255, 100), 3)
            CheckKeyBtn.Text = "LOADING..."
            
            task.wait(1.5)
            ScreenGui:Destroy()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/MineRoma/MRHUBNEW_UNI/refs/heads/main/nnnft'))()
        else
            showNotification("Invalid Key", message, Color3.fromRGB(255, 50, 50), 4)
            CheckKeyBtn.Text = "CHECK KEY"
        end
    end)
end)

-- =========================================================================
-- !!! CHECK SAVED KEY ON START !!!
-- =========================================================================
task.spawn(function()
    task.wait(0.5)
    
    local savedKey = loadKey()
    
    if savedKey and savedKey ~= "" then
        showNotification("Checking Key", "Verifying saved key...", Color3.fromRGB(161, 46, 255), 2)
        
        task.wait(0.5)
        
        local success, message = verifyKey(savedKey)
        
        if success then
            showNotification("Welcome Back!", "Key still valid. Loading...", Color3.fromRGB(0, 255, 100), 2)
            
            task.wait(1.5)
            ScreenGui:Destroy()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/MineRoma/MRHUBNEW_UNI/refs/heads/main/nnnft'))()
        else
            showNotification("Key Expired", "Your saved key has expired. Get a new one!", Color3.fromRGB(255, 150, 0), 4)
            TextBox.Text = savedKey
        end
    else
        showNotification("MR HUB", "Welcome! Get your key to continue.", Color3.fromRGB(161, 46, 255), 4)
    end
end)
