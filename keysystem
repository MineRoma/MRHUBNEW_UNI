-- Character limit and auto text size
local maxChars = 30
local baseTextSize = isMobile and 14 or 16
local minTextSize = isMobile and 10 or 12

TextBox:GetPropertyChangedSignal("Text"):Connect(function()
    -- Character limit
    if #TextBox.Text > maxChars then
        TextBox.Text = string.sub(TextBox.Text, 1, maxChars)
    end
    
    -- Auto-size text based on length
    local textLength = #TextBox.Text
    if textLength > 20 then
        local scale = 1 - ((textLength - 20) / (maxChars - 20)) * 0.3
        TextBox.TextSize = math.max(minTextSize, baseTextSize * scale)
    else
        TextBox.TextSize = baseTextSize
    end
end)

