-- =========================================================================
-- !!! –§–ò–ö–° –ó–ê–ì–†–£–ó–ö–ò: –£–î–ê–õ–ï–ù –ü–†–û–ë–õ–ï–ú–ù–´–ô –ü–ê–†–ê–ú–ï–¢–† –ò–ó GET !!!
-- =========================================================================
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/MineRoma/MRHUBNEW_UNI/refs/heads/main/guimod'))()

-- =========================================================================
-- !!! –ì–õ–û–ë–ê–õ–¨–ù–´–ï –ù–ê–°–¢–†–û–ô–ö–ò –§–ê–†–ú–ê –ò –ü–ï–†–ï–ú–ï–ù–ù–´–ï GUI !!!
-- =========================================================================
local auto = 0
local smartFarmActive = false 
-- **–î–í–ï –ì–õ–û–ë–ê–õ–¨–ù–´–ï –ü–ï–†–ï–ú–ï–ù–ù–´–ï** –¥–ª—è –≤—ã–±–æ—Ä–∞
selectedCase = "Gold" 
selectedRuns = 1 -- –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é —Ñ–∞—Ä–º–∏–º 10 Gold –∫–µ–π—Å–æ–≤

-- –¶–µ–Ω—ã –Ω–∞ 10 –∫–µ–π—Å–æ–≤ 
local CASE_PRICES = {
    ["Gold"] = 19990, 
    ["Manager"] = 8350,
    ["Office Clerk"] = 6890,
    ["Plodder"] = 4440,
    ["Beggar"] = 2200,
    ["Trash"] = 0, -- Trash –≤—Å–µ–≥–¥–∞ –±–µ—Å–ø–ª–∞—Ç–Ω—ã–π
}

-- –ü—Ä–∏–æ—Ä–∏—Ç–µ—Ç –∫–µ–π—Å–æ–≤
local CASE_PRIORITY = {
    "Gold", "Manager", "Office Clerk", "Plodder", "Beggar", "Trash"
}

-- –°–ø–∏—Å–æ–∫ –¥–æ–ø—É—Å—Ç–∏–º—ã—Ö –∫–µ–π—Å–æ–≤
local VALID_CASES = {
    ["Gold"] = true,
    ["Manager"] = true,
    ["Office Clerk"] = true,
    ["Plodder"] = true,
    ["Beggar"] = true,
    ["Trash"] = true,
}

-- !!! –ù–û–í–´–ï –ì–õ–û–ë–ê–õ–¨–ù–´–ï –ü–ï–†–ï–ú–ï–ù–ù–´–ï –î–õ–Ø ANTI-AFK !!!
local antiAFKActive = false
local antiAFKThread = nil

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø –ü–û–õ–£–ß–ï–ù–ò–Ø –ë–ê–õ–ê–ù–°–ê (–ù–ê–î–ï–ñ–ù–ê–Ø) !!!
-- =========================================================================
local function getPlayerMoney()
    local player = game.Players.LocalPlayer
    local moneyValue = 0

    local success, moneyObject = pcall(function()
        return player.PlayerGui.MainGui.TopFrame.Balance.SubVal
    end)
    
    if success and moneyObject then
        local value = tonumber(moneyObject.Value)
        if value ~= nil then
            moneyValue = value
        end
    end

    return moneyValue 
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø –û–°–ù–û–í–ù–û–ì–û –¶–ò–ö–õ–ê SMART FARM (–° –ó–ê–î–ï–†–ñ–ö–û–ô 0.5) !!!
-- =========================================================================
local smartFarmThread = nil 
local function runSmartFarm()
    while smartFarmActive do
        local currentMoney = getPlayerMoney()
        local caseFound = false
        
        for _, caseName in ipairs(CASE_PRIORITY) do
            local cost = CASE_PRICES[caseName]
            
            if currentMoney >= cost then
                
                local argsOpen = { caseName, 10 }
                game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("OpenCase"):InvokeServer(unpack(argsOpen))
                
                wait(0.5) 
                
                local argsSell = { "Sell", "ALL" }
                game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Inventory"):FireServer(unpack(argsSell))
                
                Rayfield:Notify({
                   Title = "AutoFarm Stars", 
                   Content = "–ö—É–ø–ª–µ–Ω –∫–µ–π—Å: "..caseName,
                   Duration = 1.0,
                })

                caseFound = true
                break 
            end
        end
        
        if not caseFound then
            Rayfield:Notify({
               Title = "AutoFarm Stars",
               Content = "–ù–∏—â–∏—Ç–∞ —É —Ç–µ–±—è –¥–∞–∂–µ –Ω–µ—Ç –¥–µ–Ω–µ–≥ –Ω–∞ begger!. –ù—É –≤—Å—ë –∂–¥–∏ 3 —Å–µ–∫—É–Ω–¥—ã.",
               Duration = 3.0,
            })
            wait(3) 
        else
            wait(1.5)
        end
    end
end

-- =========================================================================
-- !!! –ù–û–í–ê–Ø –§–£–ù–ö–¶–ò–Ø ANTI-AFK !!!
-- =========================================================================
local function runAntiAFK()
    local player = game.Players.LocalPlayer
    while antiAFKActive do
        local character = player.Character
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        
        if humanoid then
            humanoid.Jump = true -- –ó–∞—Å—Ç–∞–≤–ª—è–µ–º –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –ø—Ä—ã–≥–Ω—É—Ç—å
        end
        
        wait(4) -- –ñ–¥–µ–º 4 —Å–µ–∫—É–Ω–¥—ã –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º –ø—Ä—ã–∂–∫–æ–º
    end
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ú–ê–ù–£–ê–õ–¨–ù–û–ì–û –§–ê–†–ú–ê (–ò—Å–ø–æ–ª—å–∑—É–µ—Ç selectedCase –∏ selectedRuns) !!!
-- =========================================================================
local function runManualCaseFarm()
    auto = 0
    local runsToBreak = selectedRuns
    local caseName = selectedCase
    
    -- –£—Å–∏–ª–µ–Ω–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞
    if type(caseName) ~= "string" or not VALID_CASES[caseName] or type(runsToBreak) ~= "number" or runsToBreak < 1 then
        Rayfield:Notify({
            Title = "Farm Case", 
            Content = "–¢—ã –Ω–µ –ø—Ä–∞–≤–∏–ª—å–Ω–æ –≤—ã–±—Ä–∞–ª –±–µ–∑–¥–∞—Ä—å. –í—ã–±–µ—Ä–∏ —É–∂–µ –∫–µ–π—Å –∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ.", 
            Duration = 4.0,
        })
        return
    end

    -- runsToBreak - —ç—Ç–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ü–∏–∫–ª–æ–≤, –∫–∞–∂–¥—ã–π —Ü–∏–∫–ª –æ—Ç–∫—Ä—ã–≤–∞–µ—Ç 10 –∫–µ–π—Å–æ–≤.
    while true do
        local argsOpen = { caseName, 10 }
        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("OpenCase"):InvokeServer(unpack(argsOpen))
        
        wait(0.5) 
        
        local argsSell = { "Sell", "ALL" }
        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Inventory"):FireServer(unpack(argsSell))
        
        wait(1)
        auto += 1
        if auto == runsToBreak then auto = 0; break end
    end
    
    Rayfield:Notify({
        Title = "Farm case",
        Content = string.format("–ù—É —è —É–∂–µ –Ω–∞—Ñ–∞—Ä–º–∏–ª %d –∫–µ–π—Å–æ–≤ %s.", runsToBreak * 10, caseName),
        Duration = 3.0,
    })
end


