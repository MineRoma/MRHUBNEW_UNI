-- =========================================================================
-- !!! –§–ò–ö–° –ó–ê–ì–†–£–ó–ö–ò: –£–î–ê–õ–ï–ù –ü–†–û–ë–õ–ï–ú–ù–´–ô –ü–ê–†–ê–ú–ï–¢–† –ò–ó GET !!!
-- =========================================================================
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/MineRoma/MRHUBNEW_UNI/refs/heads/main/guimod'))()

-- =========================================================================
-- !!! –°–ï–†–í–ò–°–´ !!!
-- =========================================================================
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")

-- =========================================================================
-- !!! –ì–õ–û–ë–ê–õ–¨–ù–´–ï –ù–ê–°–¢–†–û–ô–ö–ò –§–ê–†–ú–ê –ò –ü–ï–†–ï–ú–ï–ù–ù–´–ï GUI !!!
-- =========================================================================
local auto = 0
local smartFarmActive = false 
selectedCase = "Gold" 
selectedRuns = 1

local CASE_PRICES = {
    ["Gold"] = 19990, 
    ["Manager"] = 8350,
    ["Office Clerk"] = 6890,
    ["Plodder"] = 4440,
    ["Beggar"] = 2200,
    ["Trash"] = 0,
}

local CASE_PRIORITY = {
    "Gold", "Manager", "Office Clerk", "Plodder", "Beggar", "Trash"
}

local VALID_CASES = {
    ["Gold"] = true,
    ["Manager"] = true,
    ["Office Clerk"] = true,
    ["Plodder"] = true,
    ["Beggar"] = true,
    ["Trash"] = true,
}

local antiAFKActive = false
local antiAFKThread = nil

-- =========================================================================
-- !!! –ü–ï–†–ï–ú–ï–ù–ù–´–ï –î–õ–Ø ROCKET LIMIT !!!
-- =========================================================================
local rocketLimitActive = false
local rocketTargetMult = 1.15
local rocketThread = nil
local rocketDetected = false

-- =========================================================================
-- !!! –ü–ï–†–ï–ú–ï–ù–ù–´–ï –î–õ–Ø AUTO EXECUTION !!!
-- =========================================================================
local autoExecEnabled = false
local SCRIPT_URL = "https://raw.githubusercontent.com/MineRoma/MRHUBNEW_UNI/refs/heads/main/nftbattle"

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø –ü–†–û–í–ï–†–ö–ò –†–ï–ñ–ò–ú–ê ROCKET !!!
-- =========================================================================
local function checkRocketMode()
    local Player = Players.LocalPlayer
    local PlayerGui = Player:FindFirstChild("PlayerGui")
    
    if not PlayerGui then
        return false, "PlayerGui –Ω–µ –Ω–∞–π–¥–µ–Ω"
    end
    
    local MainGui = PlayerGui:FindFirstChild("MainGui")
    if not MainGui then
        return false, "MainGui –Ω–µ –Ω–∞–π–¥–µ–Ω"
    end
    
    local Pages = MainGui:FindFirstChild("Pages")
    if not Pages then
        return false, "Pages –Ω–µ –Ω–∞–π–¥–µ–Ω"
    end
    
    local RocketPage = Pages:FindFirstChild("Rocket")
    if not RocketPage then
        return false, "–†–µ–∂–∏–º Rocket –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –≤ —ç—Ç–æ–π –∏–≥—Ä–µ"
    end
    
    local MainFrame = RocketPage:FindFirstChild("MainFrame")
    if not MainFrame then
        return false, "MainFrame –Ω–µ –Ω–∞–π–¥–µ–Ω"
    end
    
    local RocketFrame = MainFrame:FindFirstChild("RocketFrame")
    local MultiplierLabel = MainFrame:FindFirstChild("Multiplier")
    
    if not RocketFrame or not MultiplierLabel then
        return false, "–≠–ª–µ–º–µ–Ω—Ç—ã Rocket –Ω–µ –Ω–∞–π–¥–µ–Ω—ã"
    end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤–∏–¥–∏–º–æ—Å—Ç—å
    if RocketPage.Visible then
        return true, "Rocket –∞–∫—Ç–∏–≤–µ–Ω –∏ –æ—Ç–∫—Ä—ã—Ç ‚úÖ"
    else
        return true, "Rocket –Ω–∞–π–¥–µ–Ω, –Ω–æ –Ω–µ –æ—Ç–∫—Ä—ã—Ç (–æ—Ç–∫—Ä–æ–π –≤–∫–ª–∞–¥–∫—É Rocket –≤ –∏–≥—Ä–µ)"
    end
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø QUEUE ON TELEPORT !!!
-- =========================================================================
local function setupAutoExec()
    if autoExecEnabled then
        local scriptCode = 'loadstring(game:HttpGet("' .. SCRIPT_URL .. '"))()'
        
        if queue_on_teleport then
            queue_on_teleport(scriptCode)
            return true
        elseif syn and syn.queue_on_teleport then
            syn.queue_on_teleport(scriptCode)
            return true
        elseif fluxus and fluxus.queue_on_teleport then
            fluxus.queue_on_teleport(scriptCode)
            return true
        elseif getgenv().queue_on_teleport then
            getgenv().queue_on_teleport(scriptCode)
            return true
        end
        return false
    end
    return false
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø –ü–û–õ–£–ß–ï–ù–ò–Ø –ë–ê–õ–ê–ù–°–ê (–ù–ê–î–ï–ñ–ù–ê–Ø) !!!
-- =========================================================================
local function getPlayerMoney()
    local player = game.Players.LocalPlayer
    local moneyValue = 0

    local success, moneyObject = pcall(function()
        return player.PlayerGui.MainGui.TopFrame.Balance.SubVal
    end)
    
    if success and moneyObject then
        local value = tonumber(moneyObject.Value)
        if value ~= nil then
            moneyValue = value
        end
    end

    return moneyValue 
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø ROCKET LIMIT !!!
-- =========================================================================
local function runRocketLimit()
    local Player = Players.LocalPlayer
    local PlayerGui = Player:WaitForChild("PlayerGui")
    
    local success, err = pcall(function()
        local Pages = PlayerGui:WaitForChild("MainGui", 10):WaitForChild("Pages", 10)
        local RocketPage = Pages:WaitForChild("Rocket", 10)
        local MainFrame = RocketPage:WaitForChild("MainFrame", 10)
        local RocketFrame = MainFrame:WaitForChild("RocketFrame", 10)
        local MultiplierLabel = MainFrame:WaitForChild("Multiplier", 10)
        local GiftImage = RocketFrame:WaitForChild("GiftImage", 10)
        
        local RocketRemote = ReplicatedStorage:WaitForChild("Events", 10):WaitForChild("Rocket", 10)
        
        local function getPrizeName()
            for _, v in pairs(RocketFrame:GetChildren()) do
                if v:IsA("ImageLabel") and v.Visible and v.Name ~= "GiftImage" then
                    return v.Name
                end
            end
            return GiftImage.Name
        end
        
        Rayfield:Notify({
            Title = "Rocket Limit",
            Content = "üöÄ –°–ª–µ–∂–∫–∞ –∑–∞ –º–Ω–æ–∂–∏—Ç–µ–ª–µ–º –∑–∞–ø—É—â–µ–Ω–∞. –¶–µ–ª—å: x" .. rocketTargetMult,
            Duration = 3.0,
        })
        
        while rocketLimitActive do
            local text = MultiplierLabel.Text
            local mult = tonumber(text:match("x([%d%.]+)"))
            
            if mult then
                if mult >= rocketTargetMult then
                    local prize = getPrizeName()
                    
                    Rayfield:Notify({
                        Title = "Rocket Limit",
                        Content = "‚úÖ –ü—Ä–∏–∑ –¥–æ—Å—Ç–∏–≥ x" .. mult .. " ‚Üí –∑–∞–±–∏—Ä–∞–µ–º: " .. prize,
                        Duration = 2.0,
                    })
                    
                    pcall(function()
                        RocketRemote:InvokeServer(prize, "Claim")
                    end)
                    
                    task.wait(0.15)
                    
                    if autoExecEnabled then
                        local queued = setupAutoExec()
                        if queued then
                            Rayfield:Notify({
                                Title = "AutoExecution",
                                Content = "üì¶ –°–∫—Ä–∏–ø—Ç –ø–æ—Å—Ç–∞–≤–ª–µ–Ω –≤ –æ—á–µ—Ä–µ–¥—å –Ω–∞ —Ç–µ–ª–µ–ø–æ—Ä—Ç!",
                                Duration = 2.0,
                            })
                        end
                    end
                    
                    local placeId = game.PlaceId
                    Rayfield:Notify({
                        Title = "Rocket Limit",
                        Content = "üîÅ –í—ã–ø–æ–ª–Ω—è–µ–º —Ä–µ–∫–æ–Ω–Ω–µ–∫—Ç...",
                        Duration = 2.0,
                    })
                    
                    pcall(function()
                        TeleportService:Teleport(placeId, Player)
                    end)
                    
                    task.wait(10)
                end
                
                if mult <= 1.00 and text ~= "x1.00" then
                    Rayfield:Notify({
                        Title = "Rocket Limit",
                        Content = "üí• –†–∞—É–Ω–¥ –ø—Ä–æ–∏–≥—Ä–∞–Ω. –ñ–¥—ë–º –Ω–æ–≤—ã–π —Å—Ç–∞—Ä—Ç...",
                        Duration = 2.0,
                    })
                    task.wait(4)
                end
            end
            
            task.wait(0.2)
        end
    end)
    
    if not success then
        Rayfield:Notify({
            Title = "Rocket Limit",
            Content = "‚ùå –û—à–∏–±–∫–∞: " .. tostring(err) .. "\n–í–æ–∑–º–æ–∂–Ω–æ —Ç—ã –Ω–µ –≤ —Ä–µ–∂–∏–º–µ Rocket!",
            Duration = 5.0,
        })
        rocketLimitActive = false
    end
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø –û–°–ù–û–í–ù–û–ì–û –¶–ò–ö–õ–ê SMART FARM (–° –ó–ê–î–ï–†–ñ–ö–û–ô 0.5) !!!
-- =========================================================================
local smartFarmThread = nil 
local function runSmartFarm()
    while smartFarmActive do
        local currentMoney = getPlayerMoney()
        local caseFound = false
        
        for _, caseName in ipairs(CASE_PRIORITY) do
            local cost = CASE_PRICES[caseName]
            
            if currentMoney >= cost then
                
                local argsOpen = { caseName, 10 }
                game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("OpenCase"):InvokeServer(unpack(argsOpen))
                
                wait(0.5) 
                
                local argsSell = { "Sell", "ALL" }
                game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Inventory"):FireServer(unpack(argsSell))
                
                Rayfield:Notify({
                   Title = "AutoFarm Stars", 
                   Content = "–ö—É–ø–ª–µ–Ω –∫–µ–π—Å: "..caseName,
                   Duration = 1.0,
                })

                caseFound = true
                break 
            end
        end
        
        if not caseFound then
            Rayfield:Notify({
               Title = "AutoFarm Stars",
               Content = "–ù–∏—â–∏—Ç–∞ —É —Ç–µ–±—è –¥–∞–∂–µ –Ω–µ—Ç –¥–µ–Ω–µ–≥ –Ω–∞ begger!. –ù—É –≤—Å—ë –∂–¥–∏ 3 —Å–µ–∫—É–Ω–¥—ã.",
               Duration = 3.0,
            })
            wait(3) 
        else
            wait(1.5)
        end
    end
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø ANTI-AFK !!!
-- =========================================================================
local function runAntiAFK()
    local player = game.Players.LocalPlayer
    while antiAFKActive do
        local character = player.Character
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        
        if humanoid then
            humanoid.Jump = true
        end
        
        wait(4)
    end
end

-- =========================================================================
-- !!! –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ú–ê–ù–£–ê–õ–¨–ù–û–ì–û –§–ê–†–ú–ê !!!
-- =========================================================================
local function runManualCaseFarm()
    auto = 0
    local runsToBreak = selectedRuns
    local caseName = selectedCase
    
    if type(caseName) ~= "string" or not VALID_CASES[caseName] or type(runsToBreak) ~= "number" or runsToBreak < 1 then
        Rayfield:Notify({
            Title = "Farm Case", 
            Content = "–¢—ã –Ω–µ –ø—Ä–∞–≤–∏–ª—å–Ω–æ –≤—ã–±—Ä–∞–ª –±–µ–∑–¥–∞—Ä—å. –í—ã–±–µ—Ä–∏ —É–∂–µ –∫–µ–π—Å –∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ.", 
            Duration = 4.0,
        })
        return
    end

    while true do
        local argsOpen = { caseName, 10 }
        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("OpenCase"):InvokeServer(unpack(argsOpen))
        
        wait(0.5) 
        
        local argsSell = { "Sell", "ALL" }
        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Inventory"):FireServer(unpack(argsSell))
        
        wait(1)
        auto += 1
        if auto == runsToBreak then auto = 0; break end
    end
    
    Rayfield:Notify({
        Title = "Farm case",
        Content = string.format("–ù—É —è —É–∂–µ –Ω–∞—Ñ–∞—Ä–º–∏–ª %d –∫–µ–π—Å–æ–≤ %s.", runsToBreak * 10, caseName),
        Duration = 3.0,
    })
end


