-- =========================================================================
-- !!! –í–ö–õ–ê–î–ö–ê Rocket Limit üöÄ !!!
-- =========================================================================
TabRocket:CreateLabel("üöÄ –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –ª–∏–º–∏—Ç–∞ Rocket")

-- –°—Ç–∞—Ç—É—Å Rocket —Ä–µ–∂–∏–º–∞
local rocketModeLabel = TabRocket:CreateLabel("üîç –°—Ç–∞—Ç—É—Å: –ü—Ä–æ–≤–µ—Ä–∫–∞...")

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ä–µ–∂–∏–º –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ
task.spawn(function()
    task.wait(1)
    local found, message = checkRocketMode()
    rocketDetected = found
    if found then
        rocketModeLabel:Set("üü¢ " .. message)
        Rayfield:Notify({
            Title = "Rocket Limit",
            Content = "üöÄ –£–≤–∏–¥–µ–ª —Ç–æ —á—Ç–æ —Ç—ã –∑–∞—à—ë–ª –≤ —Ä–∞–∫–µ—Ç–∫—É! –¢–µ–ø–µ—Ä—å —Å–º–µ–ª–æ –º–æ–∂–µ—à—å —Å—Ç–∞–≤–∏—Ç—å –ª–∏–º–∏—Ç.",
            Duration = 3.0,
        })
    else
        rocketModeLabel:Set("üî¥ " .. message)
        Rayfield:Notify({
            Title = "Rocket Limit",
            Content = "‚ùå " .. message,
            Duration = 3.0,
        })
    end
end)

TabRocket:CreateButton({
    Name = "üîÑ –ü—Ä–æ–≤–µ—Ä–∏—Ç—å —Ä–µ–∂–∏–º Rocket",
    Callback = function()
        local found, message = checkRocketMode()
        rocketDetected = found
        if found then
            rocketModeLabel:Set("üü¢ " .. message)
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "üöÄ " .. message,
                Duration = 3.0,
            })
        else
            rocketModeLabel:Set("üî¥ " .. message)
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "‚ùå " .. message,
                Duration = 3.0,
            })
        end
    end,
})

TabRocket:CreateLabel("")
TabRocket:CreateLabel("–í–≤–µ–¥–∏ –º–Ω–æ–∂–∏—Ç–µ–ª—å –ø—Ä–∏ –∫–æ—Ç–æ—Ä–æ–º –∑–∞–±—Ä–∞—Ç—å –ø—Ä–∏–∑:")

local rocketStatusLabel = TabRocket:CreateLabel("–¢–µ–∫—É—â–∏–π –ª–∏–º–∏—Ç: x" .. rocketTargetMult .. " | –°—Ç–∞—Ç—É—Å: –í—ã–∫–ª—é—á–µ–Ω")

local function updateRocketStatus()
    local status = rocketLimitActive and "–í–∫–ª—é—á–µ–Ω ‚úÖ" or "–í—ã–∫–ª—é—á–µ–Ω ‚ùå"
    rocketStatusLabel:Set("–¢–µ–∫—É—â–∏–π –ª–∏–º–∏—Ç: x" .. rocketTargetMult .. " | –°—Ç–∞—Ç—É—Å: " .. status)
end

TabRocket:CreateInput({
    Name = "–ú–Ω–æ–∂–∏—Ç–µ–ª—å (–º–∏–Ω–∏–º—É–º 1.01)",
    PlaceholderText = "1.15",
    RemoveTextAfterFocusLost = false,
    Callback = function(Text)
        local num = tonumber(Text)
        if num and num >= 1.01 then
            rocketTargetMult = num
            updateRocketStatus()
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "–£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –ª–∏–º–∏—Ç: x" .. num,
                Duration = 2.0,
            })
        else
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "–û—à–∏–±–∫–∞! –í–≤–µ–¥–∏ —á–∏—Å–ª–æ –Ω–µ –º–µ–Ω–µ–µ 1.01",
                Duration = 2.0,
            })
        end
    end,
})

TabRocket:CreateButton({
    Name = "üéØ –ü–æ—Å—Ç–∞–≤–∏—Ç—å –ª–∏–º–∏—Ç",
    Callback = function()
        -- –°–Ω–∞—á–∞–ª–∞ –ø—Ä–æ–≤–µ—Ä—è–µ–º —Ä–µ–∂–∏–º
        local found, message = checkRocketMode()
        rocketDetected = found
        rocketModeLabel:Set(found and ("üü¢ " .. message) or ("üî¥ " .. message))
        
        if not found then
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "‚ùå " .. message .. "\n–î–ª—è –Ω–∞—á–∞–ª–∞ –∑–∞–π—Ç–∏ –≤ Rocket!",
                Duration = 4.0,
            })
            return
        end
        
        if not rocketLimitActive then
            if rocketTargetMult >= 1.01 then
                rocketLimitActive = true
                updateRocketStatus()
                rocketThread = task.spawn(runRocketLimit)
                Rayfield:Notify({
                    Title = "Rocket Limit",
                    Content = "‚úÖ –õ–∏–º–∏—Ç x" .. rocketTargetMult .. " —É—Å–ø–µ—à–Ω–æ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!",
                    Duration = 2.0,
                })
            else
                Rayfield:Notify({
                    Title = "Rocket Limit",
                    Content = "–í–≤–µ–¥–∏ –¥–ª—è –Ω–∞—á–∞–ª–∞ –ª–∏–º–∏—Ç –º–Ω–æ–∂–∏—Ç–µ–ª—è (–º–∏–Ω–∏–º—É–º 1.01)!",
                    Duration = 2.0,
                })
            end
        else
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "–õ–∏–º–∏—Ç –∫–∞–∫ –±—ã —É–∂–µ –∞–∫—Ç–∏–≤–µ–Ω, —á—Ç–æ–±—ã —Å–º–µ–Ω–∏—Ç—å –ø—Ä–æ—Å—Ç–æ –æ—Ç–∫–ª—é—á–∏ –ª–∏–º–∏—Ç.",
                Duration = 2.0,
            })
        end
    end,
})

TabRocket:CreateButton({
    Name = "‚õî –û—Ç–∫–ª—é—á–∏—Ç—å –ª–∏–º–∏—Ç -_-",
    Callback = function()
        if rocketLimitActive then
            rocketLimitActive = false
            rocketThread = nil
            updateRocketStatus()
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "–£—Å–ø–µ—à–Ω–æ –æ—Ç–∫–ª—é—á–∏–ª –ª–∏–º–∏—Ç. 0_0",
                Duration = 2.0,
            })
        else
            Rayfield:Notify({
                Title = "Rocket Limit",
                Content = "–¢—ã –Ω–µ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–ª –ª–∏–º–∏—Ç. -_-",
                Duration = 2.0,
            })
        end
    end,
})

