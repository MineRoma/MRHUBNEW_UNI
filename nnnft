      Invite = "noinvitelink", 
      RememberJoins = true 
   },
   KeySystem = true, 
   KeySettings = {
      Title = "Key System",
      Subtitle = "@minrteam",
      Note = "–ö–ª—é—á @minrteam –∏ –ø–∏—Å–∞—Ç—å —Å–æ–∑–¥–∞—Ç–µ–ª—é", 
      FileName = "BetaKey", 
      SaveKey = true, 
      GrabKeyFromSite = false, 
      Key = {"BetaDev"} 
   }
})

-- =========================================================================
-- !!! –°–û–ó–î–ê–ù–ò–ï –í–ö–õ–ê–î–û–ö (–¢–µ–ø–µ—Ä—å 3 –≤–∫–ª–∞–¥–∫–∏) !!!
-- =========================================================================
local TabSmart = Window:CreateTab("AutoFarm Stars", "dollar-sign")
local TabCaseFarm = Window:CreateTab("Farm case", "box")
local Tab2 = Window:CreateTab("Info", "book-text") 
local Tab1 = Window:CreateTab("Credits", "creative-commons") 

Rayfield:Notify({
   Title = "–í–ù–ò–ú–ê–ù–ò–ï",
   Content = "–í–æ–∑–º–æ–∂–Ω—ã –±–∞–≥–∏",
   Duration = 6.5,
   Image = "x",
})

-- =========================================================================
-- !!! –í–ö–õ–ê–î–ö–ê Case Farm üì¶ (–ö–ù–û–ü–ö–ò) !!!
-- =========================================================================
local currentStatusLabel = TabCaseFarm:CreateLabel(string.format("–¢—ã –≤—ã–±—Ä–∞–ª —Ñ–∞—Ä–º: –∫–µ–π—Å: %s | —Ü–∏–∫–ª–æ–≤: %d", selectedCase, selectedRuns))

-- *** –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞ ***
local function updateStatusLabel()
    currentStatusLabel:Set(string.format("–¢—ã –≤—ã–±—Ä–∞–ª —Ñ–∞—Ä–º: –∫–µ–π—Å: %s | —Ü–∏–∫–ª–æ–≤: %d", selectedCase, selectedRuns))
end

-- *** –°–ï–ö–¶–ò–Ø 1: –í–´–ë–û–† –ö–ï–ô–°–ê ***
TabCaseFarm:CreateLabel("–í—ã–±–µ—Ä–∏ –∫–µ–π—Å –∫–æ—Ç–æ—Ä—ã–π —Ö–æ—á–µ—à—å —Ñ–∞—Ä–º–∏—Ç—å:")

local function createCaseSelectCallback(caseName)
    return function()
        local success, err = pcall(function()
            selectedCase = caseName
            updateStatusLabel()
            Rayfield:Notify({Title = "Farm case: ", Content = "–ê —Ç—ã –∑–Ω–∞–ª —á—Ç–æ —Ç—ã –≤—ã–±—Ä–∞–ª –∫–µ–π—Å: "..caseName, Duration = 2.0})
        end)
        if not success then Rayfield:Notify({Title = "Farm case –û–®–ò–ë–ö–ê", Content = "–î–ê –Å–ú–ê–Å, —Å–±–æ–π –ø—Ä–∏ –≤—ã–±–æ—Ä–µ –∫–µ–π—Å–∞: "..tostring(err), Duration = 3.0}) end
    end
end

TabCaseFarm:CreateButton({ Name = "Gold", Callback = createCaseSelectCallback("Gold") })
TabCaseFarm:CreateButton({ Name = "Manager", Callback = createCaseSelectCallback("Manager") })
TabCaseFarm:CreateButton({ Name = "Office Clerk", Callback = createCaseSelectCallback("Office Clerk") })
TabCaseFarm:CreateButton({ Name = "Plodder", Callback = createCaseSelectCallback("Plodder") })
TabCaseFarm:CreateButton({ Name = "Beggar", Callback = createCaseSelectCallback("Beggar") })
TabCaseFarm:CreateButton({ Name = "Trash", Callback = createCaseSelectCallback("Trash") })


-- TabCaseFarm:CreateLabel("---")

-- *** –°–ï–ö–¶–ò–Ø 2: –í–´–ë–û–† –ö–û–õ–ò–ß–ï–°–¢–í–ê –ò –ó–ê–ü–£–°–ö ***
TabCaseFarm:CreateLabel("–í—ã–±–µ—Ä–∞–π –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ —Ñ–∞—Ä–º–∞ –∫–µ–π—Å–æ–≤:")

local function createRunsSelectCallback(runs)
    return function()
        local success, err = pcall(function()
            selectedRuns = runs
            updateStatusLabel()
            Rayfield:Notify({Title = "Case Farm –í—ã–±–æ—Ä", Content = string.format("–ë—Ä–æ, —Ç—ã –≤—ã–±—Ä–∞–ª %d —Ü–∏–∫–ª–æ–≤ (%d –∫–µ–π—Å–æ–≤)", runs, runs * 10), Duration = 2.0})
        end)
        if not success then Rayfield:Notify({Title = "Case Farm –û–®–ò–ë–ö–ê", Content = "–ù—É –±–ª—è—Ç—å, –æ—à–∏–±–∫–∞ –ø—Ä–∏ –≤—ã–±–æ—Ä–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ —Ñ–∞—Ä–º–∞: "..tostring(err), Duration = 3.0}) end
    end
end

TabCaseFarm:CreateButton({ Name = "1–ú–õ–ù –∫–µ–π—Å–æ–≤ (100000 —Ü–∏–∫–ª–æ–≤)", Callback = createRunsSelectCallback(100000) })
TabCaseFarm:CreateButton({ Name = "100–ö –∫–µ–π—Å–æ–≤ (10000 —Ü–∏–∫–ª–æ–≤)", Callback = createRunsSelectCallback(10000) })
TabCaseFarm:CreateButton({ Name = "10–ö –∫–µ–π—Å–æ–≤ (1000 —Ü–∏–∫–ª–æ–≤)", Callback = createRunsSelectCallback(1000) })
TabCaseFarm:CreateButton({ Name = "1–ö –∫–µ–π—Å–æ–≤ (100 —Ü–∏–∫–ª–æ–≤)", Callback = createRunsSelectCallback(100) })
TabCaseFarm:CreateButton({ Name = "100 –∫–µ–π—Å–æ–≤ (10 —Ü–∏–∫–ª–æ–≤)", Callback = createRunsSelectCallback(10) })
TabCaseFarm:CreateButton({ Name = "10 –∫–µ–π—Å–æ–≤ (1 —Ü–∏–∫–ª)", Callback = createRunsSelectCallback(1) })

TabCaseFarm:CreateLabel("")

-- *** –°–ï–ö–¶–ò–Ø 3: –ó–ê–ü–£–°–ö –§–ê–†–ú–ê ***
TabCaseFarm:CreateButton({ 
    Name = "<_> Start Farm Case <_>", 
    Callback = function()
        local success, err = pcall(runManualCaseFarm)
        if not success then
             Rayfield:Notify({
                Title = "–ö–†–ò–¢–ò–ß–ï–°–ö–ê–Ø –û–®–ò–ë–ö–ê –§–ê–†–ú–ê", 
                Content = "–ù—É –∫–æ—Ä–æ—á–µ –æ—à–∏–±–∫–∞ –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ: "..tostring(err),
                Duration = 5.0,
             })
        end
    end
})


-- =========================================================================
-- !!! –í–ö–õ–ê–î–ö–ê AutoFarm Stars (–û–†–ò–ì–ò–ù–ê–õ–¨–ù–´–ô –ö–û–î + ANTI-AFK) !!!
-- =========================================================================

TabSmart:CreateLabel("Farm stars")
TabSmart:CreateButton({
   Name = "–ó–∞–ø—É—Å—Ç–∏—Ç—å AutoFarm Stars",
   Callback = function()
       if not smartFarmActive then
            smartFarmActive = true
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "AutoFarm Stars –∑–∞–ø—É—â–µ–Ω! –°—Ç–∞—Ä–∞—é—Å—å –±—ã—Ç—å –Ω–µ –±–µ–∑–¥–∞—Ä–µ–º –∏ —Ñ–∞—Ä–º–∏—Ç—å –Ω–æ—Ä–º–∞–ª—å–Ω–æ.", Duration = 3})
            smartFarmThread = spawn(runSmartFarm) 
        else
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "AutoFarm Stars –±—ã–ª —É–∂–µ –∑–∞–ø—É—â–µ–Ω! –î—É—Ä–∞—á–µ–ª–æ.", Duration = 2})
        end
   end,
})

TabSmart:CreateButton({
   Name = "–û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å AutoFarm Stars",
   Callback = function()
        if smartFarmActive then
            smartFarmActive = false
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "AutoFarm Stars –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω :(.", Duration = 2})
            smartFarmThread = nil
        else
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "–ß—Ç–æ —Ç–æ —Ç—É—Ç –Ω–µ —Ç–∞–∫... –ë—Ä–æ —Ç—ã –µ–≥–æ –¥–∞–∂–µ –Ω–µ –∑–∞–ø—É—Å–∫–∞–ª.", Duration = 2})
        end
   end,
})

---
-- !!! –ù–û–í–ê–Ø –ö–ù–û–ü–ö–ê ANTI-AFK !!!
TabSmart:CreateLabel("–ü–æ–ª–µ–∑–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è AutoFarm Stars")

TabSmart:CreateToggle({
    Name = "Anti-AFK",
    Callback = function(state)
        antiAFKActive = state
        if state then
            -- –ï—Å–ª–∏ –≤–∫–ª—é—á–µ–Ω–æ, –∑–∞–ø—É—Å–∫–∞–µ–º –ø–æ—Ç–æ–∫
            antiAFKThread = spawn(runAntiAFK)
            Rayfield:Notify({Title = "Anti-Afk", Content = "–ù—É –Ω–∞–≤–µ—Ä–Ω–æ–µ —Ç–µ–±—è –Ω–µ –∫–∏–∫–Ω–µ—Ç –∑–∞ 20 –º–∏–Ω—É—Ç –∞—Ñ–∫ (–¥–æ–ª–∂–Ω–æ).", Duration = 3})
        else
            -- –ï—Å–ª–∏ –≤—ã–∫–ª—é—á–µ–Ω–æ, –æ—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–æ—Ç–æ–∫ (–ø—Ä–æ—Å—Ç–æ –æ–±–Ω—É–ª—è–µ–º, —Ç.–∫. —Ü–∏–∫–ª —Å–∞–º –∑–∞–≤–µ—Ä—à–∏—Ç—Å—è)
            antiAFKThread = nil
            Rayfield:Notify({Title = "Anti-Afk", Content = "–ù—É –∫–æ—Ä–æ—á–µ –≤—Ä–æ–¥–µ –∞–Ω—Ç–∏ –∞—Ñ–∫ –≤—ã–∫–ª—é—á–µ–Ω.", Duration = 2})
        end
    end,
})

