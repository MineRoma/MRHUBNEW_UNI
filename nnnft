TabSmart:CreateLabel("Farm stars")
TabSmart:CreateButton({
   Name = "–ó–∞–ø—É—Å—Ç–∏—Ç—å AutoFarm Stars",
   Callback = function()
       if not smartFarmActive then
            smartFarmActive = true
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "AutoFarm Stars –∑–∞–ø—É—â–µ–Ω! –°—Ç–∞—Ä–∞—é—Å—å –±—ã—Ç—å –Ω–µ –±–µ–∑–¥–∞—Ä–µ–º –∏ —Ñ–∞—Ä–º–∏—Ç—å –Ω–æ—Ä–º–∞–ª—å–Ω–æ.", Duration = 3})
            smartFarmThread = spawn(runSmartFarm) 
        else
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "AutoFarm Stars –±—ã–ª —É–∂–µ –∑–∞–ø—É—â–µ–Ω! –î—É—Ä–∞—á–µ–ª–æ.", Duration = 2})
        end
   end,
})

TabSmart:CreateButton({
   Name = "–û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å AutoFarm Stars",
   Callback = function()
        if smartFarmActive then
            smartFarmActive = false
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "AutoFarm Stars –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω :(.", Duration = 2})
            smartFarmThread = nil
        else
            Rayfield:Notify({Title = "AutoFarm Stars", Content = "–ß—Ç–æ —Ç–æ —Ç—É—Ç –Ω–µ —Ç–∞–∫... –ë—Ä–æ —Ç—ã –µ–≥–æ –¥–∞–∂–µ –Ω–µ –∑–∞–ø—É—Å–∫–∞–ª.", Duration = 2})
        end
   end,
})

TabSmart:CreateLabel("–ü–æ–ª–µ–∑–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è AutoFarm Stars")

TabSmart:CreateToggle({
    Name = "Anti-AFK",
    Callback = function(state)
        antiAFKActive = state
        if state then
            antiAFKThread = spawn(runAntiAFK)
            Rayfield:Notify({Title = "Anti-Afk", Content = "–ù—É –Ω–∞–≤–µ—Ä–Ω–æ–µ —Ç–µ–±—è –Ω–µ –∫–∏–∫–Ω–µ—Ç –∑–∞ 20 –º–∏–Ω—É—Ç –∞—Ñ–∫ (–¥–æ–ª–∂–Ω–æ).", Duration = 3})
        else
            antiAFKThread = nil
            Rayfield:Notify({Title = "Anti-Afk", Content = "–ù—É –∫–æ—Ä–æ—á–µ –≤—Ä–æ–¥–µ –∞–Ω—Ç–∏ –∞—Ñ–∫ –≤—ã–∫–ª—é—á–µ–Ω.", Duration = 2})
        end
    end,
})

