-- =========================================================================
-- !!! SMART AUTO-FARM (НОВАЯ ЛОГИКА) !!!
-- =========================================================================

-- Цены на 10 кейсов, как вы указали:
-- (Trash 0, Beggar 2200, Plodder 4440, Office Clerk 6890, Manager 8350, Gold 19990)
local CASE_PRICES = {
    ["Gold"] = 19990, 
    ["Manager"] = 8350,
    ["Office Clerk"] = 6890,
    ["Plodder"] = 4440,
    ["Beggar"] = 2200,
    ["Trash"] = 0, -- Trash всегда можно купить/открыть
}

-- Список кейсов по приоритету (от самого дорогого к самому дешевому)
local CASE_PRIORITY = {
    "Gold", "Manager", "Office Clerk", "Plodder", "Beggar", "Trash"
}

local smartFarmActive = false

local ButtonSmart = TabSmart:CreateButton({
   Name = "Запустить Smart Auto-Farm",
   Callback = function(self)
        smartFarmActive = not smartFarmActive -- Переключение состояния

        if smartFarmActive then
            self:Set({Name = "Остановить Smart Auto-Farm"})
            
            while smartFarmActive do
                local player = game.Players.LocalPlayer
                
                -- ВАЖНО: Адаптируйте этот путь, если в вашей игре деньги лежат по-другому
                local leaderstats = player:FindFirstChild("leaderstats")
                local moneyValue
                if leaderstats then
                    moneyValue = leaderstats:FindFirstChild("Money") -- предполагаем, что деньги называются "Money"
                end

                local currentMoney = moneyValue and moneyValue.Value or 0
                
                local caseFound = false
                
                -- Перебор кейсов от самого дорогого к самому дешевому
                for _, caseName in ipairs(CASE_PRIORITY) do
                    local cost = CASE_PRICES[caseName]
                    
                    if currentMoney >= cost then
                        -- Деньги есть! Открываем и продаем 10 кейсов
                        
                        -- 1. Открытие 10 кейсов
                        local argsOpen = {
                            caseName,
                            10
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("OpenCase"):InvokeServer(unpack(argsOpen))
                        
                        -- 2. Продажа всего инвентаря сразу
                        local argsSell = {
                            "Sell",
                            "ALL"
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Inventory"):FireServer(unpack(argsSell))
                        
                        Rayfield:Notify({
                           Title = "Smart Farm",
                           Content = "Куплен/продан кейс: "..caseName,
                           Duration = 1.0,
                        })

                        caseFound = true
                        break -- Купили и продали, выходим из цикла For и проверяем баланс снова.
                    end
                end
                
                if not caseFound then
                    -- Если ни один кейс не удалось купить (даже Trash, если он не 0)
                    Rayfield:Notify({
                       Title = "Smart Farm",
                       Content = "Недостаточно денег для любого платного кейса. Ждем...",
                       Duration = 3.0,
                    })
                    wait(3) -- Ждем 3 секунды, чтобы накопить
                else
                    wait(1.5) -- Ожидание между успешными циклами покупки/продажи
                end
            end
            
            self:Set({Name = "Запустить Smart Auto-Farm"})
        else
            self:Set({Name = "Запустить Smart Auto-Farm"})
            Rayfield:Notify({Title = "Smart Farm", Content = "Фарм остановлен.", Duration = 2})
        end
   end,
})

